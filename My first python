     print(anything )


for anything in range(35):                                 # will return 1 through 35 ( coz it will be the previous +1) 
     print(anything + 1)                        



for anything in range(35, 40):                               # will return 35 through 39 
     print(anything )



friends = ["tara", "tira", "mara", "moira", "mo"]            # will return the length of the range
for anything in range(len(friends)):
     print(anything)
     
    
    
friends = ["tara", "tira", "mara", "moira", "mo"]            # will return the items of the list 
for anything in range(len(friends)):
     print(friends[anything])




